<% form_for @course do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :time %><br />
    <%= f.text_field :time %>
  </p>
  <p>
    <%= f.label :course_price %><br />
    <%= f.text_field :course_price %>
  </p>
  <p>
    <%= f.label :book_price %><br />
    <%= f.text_field :book_price %>
  </p>
  <p>
    <%= f.label :prereqs %><br />
    <%= f.text_area :prereqs %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :image_path %><br />
    <%= f.text_field :image_path %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
